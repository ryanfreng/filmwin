<% provide(:title, 'Edit Submission') %>
<h1>Edit Submission</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@submission) do |s| %>
      <%= render 'fields', s: s %>

      <%= s.submit "Save entry", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
