<% provide(:title, 'New Submission Quantity') %>
<h1>New submission quantity for "<%= @event.name %>"</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@submission_quantity) do |sq| %>
      <%= render 'fields', sq: sq %>

      <%= sq.submit "Add Quantity", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
