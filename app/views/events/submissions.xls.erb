<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Wave Submissions 2014">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">User</Data></Cell>
        <Cell><Data ss:Type="String">Category</Data></Cell>
        <Cell><Data ss:Type="String">Title</Data></Cell>
        <Cell><Data ss:Type="String">Client</Data></Cell>
      </Row>
    <% @submissions.each do |sub| %>
      <Row>
        <Cell><Data ss:Type="String"><%= sub.user.name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= sub.category.name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= sub.title %></Data></Cell>
        <Cell><Data ss:Type="String"><%= sub.client %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>